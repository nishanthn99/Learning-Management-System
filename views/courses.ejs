<div class="container mx-auto grid grid-cols-2 gap-4">
    <% courses.forEach(course => { %>
      <%- include('course', {
            courseName: course.name,
            educatorName: course.educatorName,
            enrolledStudents: course.enrolledStudents,
            courseId: course.id
          }) 
      %>
    <% }); %>
  </div>
  